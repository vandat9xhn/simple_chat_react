{   
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "airbnb"
    ],
    "rules": {
        "semi": 1,
        "quotes": [1, "single"],
        "arrow-spacing": [1, {"before": true, "after": true}],
        "react/prop-types": 1,
        "react/no-array-index-key": 0,
        "camelcase": 0,
        "react/jsx-filename-extension": 0,
        "react/destructuring-assignment": 0,
        "no-use-before-define": 0,
        "react/prefer-stateless-function": 0,
        "no-console": 0,
        "object-curly-newline": 0
    },
    "env":{
        "es6": true,
        "browser": true
    },
    "plugins":[
        "react",
        "jsx-a11y",
        "import"
    ]
}